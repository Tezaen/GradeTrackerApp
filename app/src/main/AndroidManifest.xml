<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gradetrackerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AssignmentMain"></activity>
        <activity android:name=".AddAssignment" />
        <activity android:name=".EditAssignment" />
        <activity android:name=".EditCourse" />
        <activity android:name=".AddCourse" />
        <activity android:name=".CoursesMain" />
        <activity android:name=".Menu" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>